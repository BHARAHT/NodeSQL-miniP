<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All Users</title>
</head>
<body>
    <h1>Display all Users</h1>

    <table>
         <tr>
      <th>ID</th><th>Name</th><th>Email</th><th>Age</th><th>Country</th>
    </tr>
    <% users.forEach(user => { %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
        <td><%= user.country %></td>
      </tr>
    <% }) %>
        
    </table>
</body>
</html>